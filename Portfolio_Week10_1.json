{
  "data": {"url": "Portfolio_Week10_1.csv"},
  "encoding": {
    "color": {
      "field":"Administration",
      "type":"nominal",
      "legend": null
    }
  },

  "height": 300,
  "width": 290,
  "background": null,

  "layer": [{

    "selection": {
      "Party": {
        "type": "single",
        "fields": ["Party"],
        "bind": {
          "input": "select",
          "options": [
            null, 
            "Republican",
            "Democrat"
          ],
          "name": "Party"
        }
      }
    },
    "transform": [
      
      {"filter": {"selection": "Party"}}
    ],

    "mark": "line",
    "encoding": {
      "x": {
        "field": "Months-Since-In-Office", 
        "type": "quantitative", 
        "title": "Months In Office"
      },
      "y": {
        "field": "GDP-Index-From-Term-Start", 
        "type": "quantitative", 
        "scale":{"domain":[95, 135]},
        "title": null

      }
    }
  },
  {
    "encoding": 
    
    {
      "x": {
        "aggregate": "max", 
        "field": "Months-Since-In-Office", 
        "type": "quantitative"
      },
      "y": {
        "aggregate": {"argmax": "Months-Since-In-Office"}, 
        "field": "GDP-Index-From-Term-Start", 
        "type": "quantitative"}
    },
    "layer": [{
      "mark": {"type": "circle"},
      "transform": [
        {"filter": {"selection": "Party"}}
      ]
    }, {
      "mark": {"type": "text", "align": "left", "dx": 4},
      "transform": [
        {"filter": {"selection": "Party"}}
      ],
      "encoding": {"text": {"field":"Administration", "type": "nominal"}}
    }]
  }],
  "config": {"view": {"stroke": null}}
    }








